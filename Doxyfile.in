# Doxyfile 1.9.1

PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "Decentralized Ad-hoc Transfer and Access Exchange System"
OUTPUT_DIRECTORY       = "@DOXYGEN_OUTPUT_DIRECTORY@"

#---------------------------------------------------------------------------
# Input files
#---------------------------------------------------------------------------
INPUT                  = @PROJECT_SOURCE_DIR@/inc \
                         @PROJECT_SOURCE_DIR@/src \
                         @PROJECT_SOURCE_DIR@/README.md

FILE_PATTERNS          = *.h *.cpp *.md
RECURSIVE              = YES

#---------------------------------------------------------------------------
# Graph generation
#---------------------------------------------------------------------------
HAVE_DOT               = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES

#---------------------------------------------------------------------------
# Output formats
#---------------------------------------------------------------------------
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
EXTRACT_ALL            = YES
QUIET                  = YES